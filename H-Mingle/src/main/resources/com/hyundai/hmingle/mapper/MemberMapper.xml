<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hyundai.hmingle.mapper.MemberMapper">

	<select id="getMember">
		SELECT id, email, nickname, introduction, image_url
		FROM member
		WHERE id=#{memberId}
	</select>
</mapper>
